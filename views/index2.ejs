  <script>
  $( document ).ready(function() {
var compare1 = []
var tds2 = []
var tds1 = []
var tds3 = []
  for (var a in aliexpress){
	tds3.push('Aliexpress <span style="font-size:48">' + aliexpress[a].price + ' $USD!</span> <img width="200px" height="200px" src="'+aliexpress[a].img+'" /><br><a href="' + aliexpress[a]['affiliate url'] + '">' + aliexpress[a].title + '<br><br></a>')
  compare1.push(aliexpress[a].price)
  }
  
  for (var e in ebay){
	tds1.push('Ebay <span style="font-size:48">' + ebay[e].price + ' $USD!</span> <img width="200px" height="200px" src="'+ebay[e].img+'" /><br><a href="' + ebay[e]['affiliate url'] + '">' + ebay[e].title + '<br><br></a>')
	var compare = (parseFloat(ebay[e].price) / parseFloat(compare1[e]) - 1 ) * 100 
	var compare2 = parseFloat(ebay[e].price) - parseFloat(compare1[e])
	tds2.push('<br><br><br><span style="font-size: 48;">Similarity: ' + (ebay[e].similarity * 100).toPrecision(4) + '%! Make ' + compare.toPrecision(4) + '% or ' + compare2.toPrecision(4) + '$!</span><br><br><br><br><br><br><br>') 
  }
  for (var t in tds1){
  $("#table").append('<tr><td>' + tds1[t] + '</td><td>' + tds2[t] + '</td><td>' + tds3[t] + '</td></tr>')
  }
  })
