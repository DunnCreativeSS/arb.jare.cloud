  function GetSortOrder(prop) {  
    return function(a, b) {  
        if (a[prop] > b[prop]) {  
            return 1;  
        } else if (a[prop] < b[prop]) {  
            return -1;  
        }  
        return 0;  
    }  
}  
  
ebay.sort(GetSortOrder("price")); //Pass the attribute to be sorted on  

aliexpress.sort(GetSortOrder("price")); //Pass the attribute to be sorted on  
  
